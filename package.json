{
  "name": "honeykingdom-server",
  "version": "0.1.0",
  "description": "HoneyKingdom Server",
  "author": "DmitryScaletta",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "heroku-postbuild": "nest build && yarn migration:run",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "docker-compose up -d test-db",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "posttest:e2e": "docker-compose stop test-db",
    "test:db": "cd test/db && docker-compose down && docker-compose up -d && sleep 5",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "yarn typeorm migration:run -- -d ./typeOrm.config.ts",
    "migration:generate": "yarn typeorm -- -d ./typeOrm.config.ts migration:generate",
    "generate:kinopoisk-types": "npx openapi-typescript https://kinopoiskapiunofficial.tech/documentation/api/openapi.json --output src/kinopoisk-api/kinopoisk-api.generated.ts"
  },
  "dependencies": {
    "@axiomhq/axiom-node": "^0.5.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@twurple/auth": "^5.2.3",
    "@twurple/chat": "^5.2.3",
    "async-mutex": "^0.4.0",
    "bitly": "^7.1.2",
    "cache-manager": "^4.1.0",
    "cheerio": "^1.0.0-rc.10",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.29.2",
    "igdb-api-types": "^0.1.0",
    "mongodb": "^3.7.0",
    "ms": "^2.1.3",
    "node-signalr": "^2.0.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-twitch-strategy": "^2.2.0",
    "pg": "^8.8.0",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.9",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.2",
    "@nestjs/testing": "^9.0.11",
    "@types/axios": "^0.14.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cheerio": "^0.22.31",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/ramda": "^0.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.2",
    "msw": "^0.47.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
